  \entry{Bourrat2015}{thesis}{}
    \name{author}{1}{}{%
      {{hash=BP}{%
         family={Bourrat},
         family_i={B\bibinitperiod},
         given={Pierrick},
         given_i={P\bibinitperiod},
      }}%
